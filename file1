git init 
git status
git add .
git commit -m ""
git commit -a -m ""
git branch <bn>
git branch
git remote add alias name <url>
git remote -v
git remote -r
git checkout <bn>
git checkout -b<new bn>
git push alias branch name
git log
git log -2
git revert commit id (revert back to the updation)
git reset
git stash
git stash pop
git stash apply
git stash drop
git rebase
git pull alias bn
git clone url
git fetch
git merge bn
git cherry-pick
git commit -amend -m "" to change the commit msg
git config --global user.name ""
git clean -n
git clean -f
git diff
git tag
git push tag tn
git tag -d tn
